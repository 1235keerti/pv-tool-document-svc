{
  "name": "document-microservice",
  "version": "1.0.0",
  "description": "Serverless framework using ExpressJS TypeScript Aws-Multipart Upload",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -b && tsc-alias",
    "prestart": "yarn build",
    "postbuild": "cp ./src/public/documentation.html ./dist/src/documentation.html",
    "start:prod": "node dist/src/index.js",
    "express:start": "yarn build && cross-env NODE_ENV=development nodemon dist/src/index.js",
    "lambda:dev": "cross-env NODE_ENV=development node blurhash-event-rule-generator.js && sls offline start --stage offline",
    "lambda:prod": "cross-env NODE_ENV=production deploy --stage",
    "tail-log": "sls logs -f app -t",
    "generate-blurhash-rule": "node blurhash-event-rule-generator.js",
    "remove-prod": "sls remove --stage prod",
    "clean": "rimraf dist .webpack .serverless",
    "prettier:fix": "prettier --write \"**/*.+(md|js|ts|json|graphql)\"",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.592.0",
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/client-secrets-manager": "^3.835.0",
    "@aws-sdk/cloudfront-signer": "^3.576.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "@aws-sdk/s3-presigned-post": "^3.592.0",
    "@aws-sdk/s3-request-presigner": "^3.592.0",
    "@sentry/node": "^7.60.0",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "blurhash": "^2.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "event-dispatch": "^0.4.1",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "express-status-monitor": "^1.3.4",
    "helmet": "^7.1.0",
    "http": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.4",
    "swagger-ui-dist": "^5.17.14",
    "tsc-alias": "^1.8.10",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@koa/cors": "^5.0.0",
    "@serverless/typescript": "^4.15.1",
    "@types/aws-lambda": "^8.10.138",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-status-monitor": "^1.3.3",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "cross-env": "^7.0.3",
    "danger": "^12.3.1",
    "danger-typescript-plugin": "^1.0.11",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "mysql": "^2.18.1",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "serverless": "^3.39.0",
    "serverless-bundle": "^6.1.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-localstack": "^1.3.1",
    "serverless-offline": "^13.6.0",
    "serverless-plugin-conditional-functions": "^1.0.8",
    "serverless-tscpaths": "^0.0.8",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "eslint"
    ],
    "**/*.ts?(x)": "() => 'npm run build-types'",
    "*.json": [
      "prettier --write"
    ]
  },
  "imports": {
    "#package.json": "./package.json"
  },
  "author": "Webelight Solutions"
}
